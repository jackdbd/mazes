{:deps {this/project {:local/root "."}}
 :min-bb-version "0.9.160"
 :paths ["src"]
 :tasks {:requires ([babashka.fs :as fs])
         deps:dev
         {:doc "Install dependencies of the dev alias"
          :task (clojure "-A:dev")}
         maze:ascii
         {:doc "Prints a maze in the terminal using ASCII art"
          :task (clojure "-M -m mazes.core")}}}
